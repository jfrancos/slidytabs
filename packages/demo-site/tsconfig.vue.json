{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "noEmit": true,
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "baseUrl": ".",
    "paths": {
      "$lib/*": ["./src/lib/*"],
      "@/*": ["./src/*"]
    }
  },
  "include": [
    ".astro/types.d.ts",
    "**/*.vue",
  ]
}