{
  "name": "astro",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "install-shadcn:svelte": "tiged huntabyte/shadcn-svelte/docs/src/lib/registry/ui/tabs src/shadcn-svelte/tabs --force",
    "install-shadcn:vue": "tiged unovue/shadcn-vue/apps/v4/registry/new-york-v4/ui/tabs src/shadcn-vue/tabs --force",
    "install-shadcn:astro": "tiged starwind-ui/starwind-ui/packages/core/src/components/tabs src/shadcn-astro/tabs --force && pnpm run fix-astro",
    "install-shadcn:react": "curl https://raw.githubusercontent.com/shadcn-ui/ui/main/apps/v4/registry/new-york-v4/ui/tabs.tsx > src/shadcn/tabs.tsx",
    "install-shadcns": "pnpm run install-shadcn:svelte && pnpm run install-shadcn:vue && pnpm run install-shadcn:astro && pnpm run install-shadcn:react",
    "fix-astro": "perl -i -ne 'print unless /data-state=\"inactive\"/' src/shadcns/astro/tabs/TabsTrigger.astro",
    "dev": "astro dev --host",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/compiler": "^2.13.0",
    "@astrojs/react": "^4.4.2",
    "@astrojs/svelte": "^7.2.5",
    "@astrojs/vue": "^5.1.4",
    "@babel/parser": "^7.28.6",
    "@nanostores/persistent": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vue/compiler-dom": "^3.5.26",
    "@vueuse/core": "^14.1.0",
    "astro": "^5.16.9",
    "clsx": "^2.1.1",
    "github-markdown-css": "^5.8.1",
    "nanostores": "^1.1.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "reka-ui": "^2.7.0",
    "remark": "^15.0.1",
    "remark-toc": "^9.0.0",
    "shiki": "^3.21.0",
    "slidytabs": "link:../lib",
    "svelte": "^5.46.3",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "typescript": "^5.9.3",
    "unocss-preset-shadcn": "^1.0.1",
    "vue": "^3.5.26",
    "zimmerframe": "^1.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@iconify/json": "^2.2.427",
    "bits-ui": "^2.15.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "tiged": "^2.12.7",
    "typescript-eslint": "^8.53.0",
    "unocss": "^66.5.12"
  }
}
