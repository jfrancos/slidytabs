{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "$lib/*": ["./src/lib/*"],
      "@/*": ["./src/*"],
      "@shadcn-svelte/*": ["./src/shadcns/svelte/*"],
      "@shadcn-vue/*": ["./src/shadcns/vue/*"],
      "@shadcn-astro/*": ["./src/shadcns/astro/*"],
      "@shadcn-react/*": ["./src/shadcns/react/*"]
    }
  }
}
